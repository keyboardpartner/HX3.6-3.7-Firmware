        Inc(cOrder);
        Inc(pCircleButton);
        Dec(cntCircleButton);
        if (cntCircleButton = 0) then
        begin
          pCircleButton := 0;
          break;
        end;
      end;
      if (saveOrder <> cOrder) then
        continue;
    end;

    if (pBox) then
    begin
      while (pBox^^.Order = cOrder) do
      begin
        if (actObjOrder <> cOrder) then // draw pressed object at the end
          DrawBox(pBox^);
        Inc(cOrder);
        Inc(pBox);
        Dec(cntBox);
        if (cntBox = 0) then
        begin
          pBox := 0;
          break;
        end;
      end;
      if (saveOrder <> cOrder) then
        continue;
    end;

    if (pCBox) then
    begin
      while (pCBox^^.Order = cOrder) do
      begin
        if (actObjOrder <> cOrder) then // draw pressed object at the end
          DrawCBox(pCBox^);
        Inc(cOrder);
