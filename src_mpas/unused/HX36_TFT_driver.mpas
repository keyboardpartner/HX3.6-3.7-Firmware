procedure Init_MCU();
begin
  // Setup FT800 SPI interface
  SPI3_Init_Advanced(_SPI_FPCLK_DIV4, _SPI_MASTER or _SPI_8_BIT or
  		   _SPI_CLK_IDLE_LOW or _SPI_FIRST_CLK_EDGE_TRANSITION or
  		   _SPI_MSB_FIRST or _SPI_SS_DISABLE or _SPI_SSM_ENABLE or _SPI_SSI_1,
  		   @_GPIO_MODULE_SPI3_PB345);
  GPIO_Digital_Output(@GPIOB_BASE, _GPIO_PINMASK_8 or _GPIO_PINMASK_9);
end;

