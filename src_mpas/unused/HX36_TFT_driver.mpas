      ppEvent := ppEvent + AEventType;
      pEvent  := ppEvent^;
    end;
    // CBoxRound
    VTFT_OT_CBOXROUND: begin
      ppEvent := TPointer(@TCBox_RoundPtr(AObjInfo^.Obj)^.OnUp);
      ppEvent := ppEvent + AEventType;
      pEvent  := ppEvent^;
    end;
    // CheckBox
    VTFT_OT_CHECKBOX: begin
      ppEvent := (@TCheckBoxPtr(AObjInfo^.Obj)^.OnUp);
      ppEvent := ppEvent + AEventType;
      pEvent  := ppEvent^;
    end;
    // EveDial
    VTFT_OT_EVEDIAL: begin
      ppEvent := (@TEveDialPtr(AObjInfo^.Obj)^.OnUp);
      ppEvent := ppEvent + AEventType;
      pEvent  := ppEvent^;
    end;
    // EveKeys
    VTFT_OT_EVEKEYS: begin
