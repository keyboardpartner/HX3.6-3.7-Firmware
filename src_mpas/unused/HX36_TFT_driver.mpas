function IsInsideObject(AObjInfo: TObjInfoPtr; X: word; Y: word): byte;
var ptrPressRect:   TRectPtr;
    ptrPressCircle: TRectPtr;
begin
  Result := 0;

  ptrPressRect   := 0;
  ptrPressCircle := 0;

  if ((AObjInfo^.HitX = X) and (AObjInfo^.HitY = Y)) then
  begin
    Result := 1;
    Exit;
  end;

  case (AObjInfo^.Type_) of
    // ButtonRound
    VTFT_OT_BUTTONROUND: begin
      ptrPressRect := TRectPtr(TPointer(@TButton_RoundPtr(AObjInfo^.Obj)^.Left));
    end;
    // Label
    VTFT_OT_LABEL: begin
      ptrPressRect := TRectPtr(TPointer(@TLabelPtr(AObjInfo^.Obj)^.Left));
    end;
    // CLabel
    VTFT_OT_CLABEL: begin
      ptrPressRect := TRectPtr(TPointer(@TCLabelPtr(AObjInfo^.Obj)^.Left));
    end;
    // CCircle
    VTFT_OT_CCIRCLE: begin
      ptrPressCircle := TRectPtr(TPointer(@TCCirclePtr(AObjInfo^.Obj)^.Left));
    end;
    // CircleButton
    VTFT_OT_CIRCLEBUTTON: begin
      ptrPressCircle := TRectPtr(TPointer(@TCircleButtonPtr(AObjInfo^.Obj)^.Left));
    end;
    // Box
    VTFT_OT_BOX: begin
      ptrPressRect := TRectPtr(TPointer(@TBoxPtr(AObjInfo^.Obj)^.Left));
    end;
    // CBox
    VTFT_OT_CBOX: begin
      ptrPressRect := TRectPtr(TPointer(@TCBoxPtr(AObjInfo^.Obj)^.Left));
    end;
    // BoxRound
    VTFT_OT_BOXROUND: begin
      ptrPressRect := TRectPtr(TPointer(@TBox_RoundPtr(AObjInfo^.Obj)^.Left));
    end;
    // CBoxRound
    VTFT_OT_CBOXROUND: begin
      ptrPressRect := TRectPtr(TPointer(@TCBox_RoundPtr(AObjInfo^.Obj)^.Left));
    end;
    // CheckBox
    VTFT_OT_CHECKBOX: begin
      ptrPressRect := TRectPtr(TPointer(@TCheckBoxPtr(AObjInfo^.Obj)^.Left));
    end;
    // EveDial
    VTFT_OT_EVEDIAL: begin
      ptrPressCircle := TRectPtr(TPointer(@TEveDialPtr(AObjInfo^.Obj)^.Left));
    end;
    // EveKeys
    VTFT_OT_EVEKEYS: begin
      ptrPressRect := TRectPtr(TPointer(@TEveKeysPtr(AObjInfo^.Obj)^.Left));
    end;
    // EveScrollBar
    VTFT_OT_EVESCROLLBAR: begin
      ptrPressRect := TRectPtr(TPointer(@TEveScrollbarPtr(AObjInfo^.Obj)^.Left));
    end;
    // EveSlider
    VTFT_OT_EVESLIDER: begin
      ptrPressRect := TRectPtr(TPointer(@TEveSliderPtr(AObjInfo^.Obj)^.Left));
    end;
    // EveText
    VTFT_OT_EVETEXT: begin
      ptrPressRect := TRectPtr(TPointer(@TEveTextPtr(AObjInfo^.Obj)^.Left));
    end;
    // EveNumber
    VTFT_OT_EVENUMBER: begin
      ptrPressRect := TRectPtr(TPointer(@TEveNumberPtr(AObjInfo^.Obj)^.Left));
    end;
  end;

  if (ptrPressRect) then
  begin
    if (ptrPressRect^.Left <= X) then
      if (ptrPressRect^.Top  <= Y) then
        if (ptrPressRect^.Left+ptrPressRect^.Width-1 >= X) then
          if (ptrPressRect^.Top+ptrPressRect^.Height-1 >= Y) then
            Result := 1;
  end
  else if (ptrPressCircle) then
  begin
    if (ptrPressCircle^.Left <= X) then
      if (ptrPressCircle^.Top  <= Y) then
        if (ptrPressCircle^.Left+ptrPressCircle^.Width*2-1 >= X) then
          if (ptrPressCircle^.Top+ptrPressCircle^.Width*2-1 >= Y) then
            Result := 1;
  end
end;

procedure DrawButtonRound(AButtonRound: TButton_RoundPtr);
var textWidth:  integer;
var textHeight: integer;
begin
  if (AButtonRound^.Visible) then
  begin
    if (AButtonRound = TButton_RoundPtr(TouchS.ActObjInfo.Obj)) then
      FT800_Canvas_BrushSingleColor(_FT800_BRUSH_STYLE_SOLID, AButtonRound^.Press_Color, AButtonRound^.Opacity)
    else 
      FT800_Canvas_BrushSingleColor(_FT800_BRUSH_STYLE_SOLID, AButtonRound^.Color, AButtonRound^.Opacity);

    FT800_Canvas_Pen(AButtonRound^.Pen_Width, AButtonRound^.Pen_Color, AButtonRound^.Opacity);

    if (AButtonRound^.Tag) then
      FT800_Canvas_Tag(AButtonRound^.Tag);

    FT800_Screen_BoxRound(AButtonRound^.Left, AButtonRound^.Top, AButtonRound^.Left+AButtonRound^.Width-1, AButtonRound^.Top+AButtonRound^.Height-1, AButtonRound^.Corner_Radius);
    if (AButtonRound^.FontHandle >= 16) then
      FT800_Canvas_FontSystem(AButtonRound^.FontHandle, AButtonRound^.Font_Color, AButtonRound^.Opacity)
    else
      FT800_Canvas_Font(AButtonRound^.FontHandle, AButtonRound^.FontName, AButtonRound^.Source, AButtonRound^.Font_Color, AButtonRound^.Opacity);
    
    textWidth  := FT800_Canvas_TextWidth(AButtonRound^.Caption);
    textHeight := FT800_Canvas_TextHeight();
    if (AButtonRound^.TextAlign = taLeft) then
      FT800_Screen_TextPos(AButtonRound^.Left+(AButtonRound^.Pen_Width)+1, AButtonRound^.Top+(AButtonRound^.Height-textHeight)/2, AButtonRound^.Caption)
    else if (AButtonRound^.TextAlign = taCenter) then
      FT800_Screen_TextPos(AButtonRound^.Left+(AButtonRound^.Width-textWidth)/2, AButtonRound^.Top+(AButtonRound^.Height-textHeight)/2, AButtonRound^.Caption)
    else if (AButtonRound^.TextAlign = taRight) then
      FT800_Screen_TextPos(AButtonRound^.Left+AButtonRound^.Width-textWidth-1, AButtonRound^.Top+(AButtonRound^.Height-textHeight)/2, AButtonRound^.Caption);
  end;
end;

