      if (ABox^.Gradient <> _FT800_BRUSH_GR_NONE) then
        FT800_Canvas_BrushGradient(_FT800_BRUSH_STYLE_SOLID, ABox^.Gradient, ABox^.Color, ABox^.ColorTo, ABox^.Opacity)
