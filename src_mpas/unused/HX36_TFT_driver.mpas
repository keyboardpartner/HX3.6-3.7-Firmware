var pCircleButton: ^code ^TCircleButton;
var pBox:          ^code ^TBox;
var pCBox:         ^code ^TCBox;
var pBoxRound:     ^code ^TBox_Round;
var pCBoxRound:    ^code ^TCBox_Round;
var pLine:         ^code ^TLine;
var pCLine:        ^code ^TCLine;
var pCheckBox:     ^code ^TCheckBox;
