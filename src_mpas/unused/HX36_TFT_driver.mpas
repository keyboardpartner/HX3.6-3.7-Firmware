        FT800_Canvas_BrushGradient(_FT800_BRUSH_STYLE_SOLID, ABox^.Gradient, ABox^.Press_Color, ABox^.Press_ColorTo, ABox^.Opacity)
      else
        FT800_Canvas_BrushSingleColor(_FT800_BRUSH_STYLE_SOLID, ABox^.Press_Color, ABox^.Opacity);
