    VTFT_OT_LABEL: begin
      ppEvent := (@TLabelPtr(AObjInfo^.Obj)^.OnUp);
      ppEvent := ppEvent + AEventType;
      pEvent  := ppEvent^;
    end;
    // CLabel
    VTFT_OT_CLABEL: begin
      ppEvent := TPointer(@TCLabelPtr(AObjInfo^.Obj)^.OnUp);
      ppEvent := ppEvent + AEventType;
      pEvent  := ppEvent^;
    end;
    // CCircle
    VTFT_OT_CCIRCLE: begin
      ppEvent := TPointer(@TCCirclePtr(AObjInfo^.Obj)^.OnUp);
      ppEvent := ppEvent + AEventType;
      pEvent  := ppEvent^;
    end;
    // CircleButton
    VTFT_OT_CIRCLEBUTTON: begin
      ppEvent := (@TCircleButtonPtr(AObjInfo^.Obj)^.OnUp);
      ppEvent := ppEvent + AEventType;
      pEvent  := ppEvent^;
    end;
    // Box
    VTFT_OT_BOX: begin
      ppEvent := (@TBoxPtr(AObjInfo^.Obj)^.OnUp);
      ppEvent := ppEvent + AEventType;
      pEvent  := ppEvent^;
    end;
    // CBox
    VTFT_OT_CBOX: begin
      ppEvent := TPointer(@TCBoxPtr(AObjInfo^.Obj)^.OnUp);
      ppEvent := ppEvent + AEventType;
      pEvent  := ppEvent^;
    end;
    // BoxRound
    VTFT_OT_BOXROUND: begin
