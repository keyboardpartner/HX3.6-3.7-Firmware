procedure DrawCheckBox(ACheckBox: TCheckBoxPtr);
var textHeight:  byte;
var textLeft:    word;
var textTop:     word;
var rectLeft:    word;
var rectTop:     word;
var checkOffset: word;
begin
  if (ACheckBox^.Visible) then
  begin
    if (ACheckBox = TCheckBoxPtr(TouchS.ActObjInfo.Obj)) then
      FT800_Canvas_BrushSingleColor(_FT800_BRUSH_STYLE_SOLID, ACheckBox^.Press_Color, ACheckBox^.Opacity)
    else 
      FT800_Canvas_BrushSingleColor(_FT800_BRUSH_STYLE_SOLID, ACheckBox^.Color, ACheckBox^.Opacity);

    FT800_Canvas_Pen(ACheckBox^.Pen_Width, ACheckBox^.Pen_Color, ACheckBox^.Opacity);

    if (ACheckBox^.FontHandle >= 16) then
      FT800_Canvas_FontSystem(ACheckBox^.FontHandle, ACheckBox^.Font_Color, ACheckBox^.Opacity)
    else
      FT800_Canvas_Font(ACheckBox^.FontHandle, ACheckBox^.FontName, ACheckBox^.Source, ACheckBox^.Font_Color, ACheckBox^.Opacity);

    if (ACheckBox^.Tag) then
      FT800_Canvas_Tag(ACheckBox^.Tag);

    textHeight  := FT800_Canvas_TextHeight();
    textTop     := ACheckBox^.Top+(ACheckBox^.Height-textHeight)/2;
    rectTop     := ACheckBox^.Top;
    checkOffset := ACheckBox^.Height/5;

    if (ACheckBox^.TextAlign = taLeft) then
    begin
      textLeft := ACheckBox^.Left+ACheckBox^.Height+4;
      rectLeft := ACheckBox^.Left;
    end
    else if (ACheckBox^.TextAlign = taRight) then
    begin
      textLeft := ACheckBox^.Left;
      rectLeft := ACheckBox^.Left+ACheckBox^.Width-ACheckBox^.Height;
    end;

    FT800_Screen_BoxRound(rectLeft, rectTop, rectLeft+ACheckBox^.Height-1, rectTop+ACheckBox^.Height-1, ACheckBox^.Corner_Radius);
    if (ACheckBox^.Checked) then
    begin
      FT800_Canvas_Pen(ACheckBox^.Height/8, ACheckBox^.Pen_Color, ACheckBox^.Opacity);
      FT800_Screen_Line(rectLeft+checkOffset+1, rectTop+ACheckBox^.Height/2, rectLeft+ACheckBox^.Height/2, rectTop+ACheckBox^.Height-checkOffset-1);
      FT800_Screen_Line(rectLeft+ACheckBox^.Height/2, rectTop+ACheckBox^.Height-checkOffset-1, rectLeft+ACheckBox^.Height-checkOffset-1, rectTop+checkOffset+1);
    end;

    FT800_Screen_TextPos(textLeft, textTop, ACheckBox^.Caption);
  end;
end;

